[
  {
    "name": "mod_php",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "X-Powered-By", "regex": "(?i)php"},
          {"type": "cookie", "regex": "(?i)PHPSESSID"}
        ]
      },
      {
        "conditions": [
          {"type": "body", "regex": "(?i)(fatal error|warning:).*on line"}
        ]
      }
    ]
  },
  {
    "name": "mod_ssl",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "Server", "regex": "(?i)(mod_ssl|openssl)"}
        ]
      },
      {
        "conditions": [
          {"type": "header", "key": "Strict-Transport-Security", "regex": ".*"}
        ]
      }
    ]
  },
  {
    "name": "mod_deflate",
    "endpoint": "/",
    "method": "GET",
    "headers": {"Accept-Encoding": "gzip, deflate"},
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "Content-Encoding", "regex": "(?i)(gzip|deflate)"}
        ]
      },
      {
        "conditions": [
          {"type": "header", "key": "Vary", "regex": "(?i)Accept-Encoding"}
        ]
      }
    ]
  },
  {
    "name": "mod_headers",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "X-Frame-Options", "regex": ".*"}
        ]
      },
      {
        "conditions": [
          {"type": "header", "key": "Content-Security-Policy", "regex": ".*"}
        ]
      },
      {
        "conditions": [
          {"type": "header", "key": "X-Content-Type-Options", "regex": "(?i)nosniff"}
        ]
      }
    ]
  },
  {
    "name": "mod_jk",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "Server", "regex": "(?i)mod_jk"}
        ]
      },
      {
        "conditions": [
          {"type": "cookie", "regex": "(?i)JSESSIONID"}
        ]
      }
    ]
  },
  {
    "name": "mod_proxy",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "Via", "regex": "(?i)Apache"}
        ]
      },
      {
        "conditions": [
          {"type": "header", "key": "X-Forwarded-For", "regex": ".*"}
        ]
      }
    ]
  },
  {
    "name": "mod_pagespeed",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "X-Mod-Pagespeed", "regex": ".*"}
        ]
      },
      {
        "conditions": [
          {"type": "header", "key": "X-Page-Speed", "regex": ".*"}
        ]
      }
    ]
  },
  {
    "name": "mod_perl",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "Server", "regex": "(?i)mod_perl"}
        ]
      }
    ]
  },
  {
    "name": "mod_wsgi",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "Server", "regex": "(?i)mod_wsgi"}
        ]
      }
    ]
  },
  {
    "name": "mod_http2",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "Upgrade", "regex": "(?i)h2c"}
        ]
      }
    ]
  },
  {
    "name": "mod_dav",
    "endpoint": "/",
    "method": "OPTIONS",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "DAV", "regex": ".*"}
        ]
      },
      {
        "conditions": [
          {"type": "header", "key": "MS-Author-Via", "regex": "(?i)DAV"}
        ]
      }
    ]
  },
  {
    "name": "mod_passenger",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "X-Powered-By", "regex": "(?i)Passenger"}
        ]
      },
      {
        "conditions": [
          {"type": "header", "key": "Server", "regex": "(?i)Passenger"}
        ]
      }
    ]
  },
  {
    "name": "mod_ratelimit",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "X-RateLimit-Limit", "regex": ".*"}
        ]
      }
    ]
  },
  {
    "name": "mod_security (Passive Detection)",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "Server", "regex": "(?i)mod_security"}
        ]
      }
    ]
  },
  {
    "name": "mod_qos",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "X-QoS-Level", "regex": ".*"}
        ]
      }
    ]
  },
  {
    "name": "mod_lua",
    "endpoint": "/",
    "method": "GET",
    "matchers": [
      {
        "conditions": [
          {"type": "header", "key": "X-Lua", "regex": ".*"}
        ]
      }
    ]
  }
]